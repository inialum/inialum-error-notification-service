{
	"name": "app",
	"type": "module",
	"private": true,
	"license": "Apache-2.0",
	"scripts": {
		"dev": "wrangler dev src/index.ts --port 7070",
		"deploy": "wrangler deploy --minify src/index.ts --keep-vars",
		"create-token": "inialum-token-generator --service-name inialum-error-notification-service --env-file ./.dev.vars",
		"typegen": "wrangler types --env-interface CloudflareBindings",
		"test": "vitest",
		"test:ci": "pnpm run test:coverage",
		"test:coverage": "vitest run --coverage",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hono/swagger-ui": "^0.5.2",
		"@hono/zod-openapi": "^1.1.5",
		"hono": "^4.10.7",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@inialum/token-generator": "^0.3.0",
		"@vitest/coverage-v8": "^4.0.14",
		"discord-api-types": "^0.38.34",
		"typescript": "^5.9.3",
		"vitest": "^4.0.14",
		"wrangler": "^4.51.0"
	}
}
