{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"typecheck": {
			"dependsOn": ["^build", "^typecheck"]
		},
		"test": {
			"cache": false,
			"persistent": true
		},
		"test:ci": {
			"outputs": ["coverage/**"],
			"dependsOn": []
		},
		"clean": {
			"cache": false
		}
	}
}
